{{debug}}

{{#each procedures}}

  <div class="section">
    <div class="title">

      <h4 class="col-sm-4">#ResetModernity! <a href="">{{@key}}</a></h4>
      <p class="col-sm-8">Tweet your contribution using <a href="">#ResetModernity!</a> hashtag <br/>and <a href="">#[procedure][piece]</a> (ex. <a href="">#D2</a>) to refer to items from the exhibition.</p>
    </div>
    {{#each this}}
      {{#newLine @index}}<div class="row line">{{/newLine}}
        <div class="col-sm-4 embedContainer">
          <a href="{{url}}" class="embed"></a>
        </div>
      {{#newLine @index}}</div>{{/newLine}}
    {{/each}}
  </div>

{{/each}}
